{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "endOfLine": "auto",
  "importOrder": [
    "^@mantine/(.*)$",
    "^@/store/(.*)$",
    "^@shared/(.*)$",
    "^@entities/(.*)$",
    "^@features/(.*)$",
    "^@widgets(.*)$",
    "^@pages(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
